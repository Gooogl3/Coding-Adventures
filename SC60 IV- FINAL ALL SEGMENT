   "source": [
    "#impute the Buyer 1 data\n",
    "for i in imputed_dic.keys():    \n",
    "    Buyer1[i.upper()] = Buyer1[i.upper()].replace(imputed_dic[i], np.nan) \n",
    "    \n",
    "for i in ('auto_score', 'crossindustryscore', 'shorttermlendingscore', 'FTScore'):\n",
    "    if i == 'FTScore':\n",
    "        Buyer1[i.upper()] = Buyer1[i.upper()].mask(((Buyer1[i.upper()] < 500) | (Buyer1[i.upper()] > 900)) , np.nan)\n",
    "    else: \n",
    "        Buyer1[i.upper()]  = Buyer1[i.upper()].mask(((Buyer1[i.upper()] <= 500) | (Buyer1[i.upper()] > 900)) , np.nan)"
   ]
