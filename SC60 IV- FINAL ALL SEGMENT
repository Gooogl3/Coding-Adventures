    "    for i in var:\n",
    "        print('Variable: ',i)        \n",
    "        bench = development_seg[i].tolist()\n",
    "        comp = Buyer1_seg[i].tolist()\n",
    "        summary_table, IV = psi(bench, comp, group=10)\n",
