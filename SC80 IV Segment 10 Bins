    "#impute the Buyer 1 data\n",
    "for i in imputed_dic.keys():    \n",
    "    Buyer1[i.upper()] = Buyer1[i.upper()].replace(imputed_dic[i], np.nan) \n",
    "    \n",
    "for i in ('auto_score', 'crossindustry_score', 'short_term_lending_score', 'bankcard_score', 'telecommunication_score'): \n",
    "    Buyer1[i.upper()] = Buyer1[i.upper()].mask(((Buyer1[i.upper()] < 501) | (Buyer1[i.upper()] > 900)) , np.nan)"
